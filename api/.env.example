# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repository_name

# Slack Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_ANALYTICS_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/ANALYTICS/WEBHOOK

# Container Registry
REGISTRY=ghcr.io

# API Configuration
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:4200","http://localhost:8080"]

# Cache and Performance
CACHE_TTL=300
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100

# Analytics
DEFAULT_LOOKBACK_MINUTES=1440
MAX_LOOKBACK_MINUTES=10080
